    agent any
       stages {
            stage('Build') {
                   steps {
                   echo "Build Successfull!"
                         }
              }
              stage('Test') {
                   steps {
                   echo "Test Successfull!"
                         }
              }
              stage('Deploy') {
                   steps {
                   echo "Deploy Successfull!"
                         }
              }
              stage('Release') {
                   steps {
                   echo "Release Successfull!"
                         }
              }
         }
}
